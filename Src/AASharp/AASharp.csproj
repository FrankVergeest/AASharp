<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net40;net45</TargetFrameworks>
    <AssemblyName>AASharp</AssemblyName>
    <RootNamespace>AASharp</RootNamespace>
    <Description>Astronomical Algorithms # (AASharp)

